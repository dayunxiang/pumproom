    <script type="text/javascript">
        function searchData() {
            //alert("hello");
            //alert(document.getElementById("date").value + " " + document.getElementById("time").value);
            //alert($("#name").val());
            //alert(document.getElementById("name").value);
            var timer = document.getElementById("date").value + " " + document.getElementById("time").value;
            $.ajax({
                url: "/RTData/GetDataByTime",  //ajax请求地址
                type: "POST",//请求方式 "POST" 或 "GET"， 默认为 "GET"。注意：其它 HTTP 请求方法，如 PUT 和 DELETE 也可以使用，但仅部分浏览器支持。
                dataType: "json",    //根据返回数据类型可以有这些类型可选：xml html script json jsonp text
                //发送到服务器的数据，可以直接传对象{a:0,b:1}，如果是get请求会自动拼接到url后面，如：&a=0&b=1
                //如果为数组，jQuery 将自动为不同值对应同一个名称。如 {foo:["bar1", "bar2"]} 转换为 "&foo=bar1&foo=bar2"。
                data: { tablename: $("#name").val(), time: timer },
                success:
                    function (result) {

                        if (!result.IsSuccess) {
                            //alert("ok");
                            //返回的json的字符串
                            //alert(result.json);
                            var str = JSON.parse(result.json);
                            //alert(str.length);
                            //清空tbody的数据
                            $("#tabletofill tbody").html("");
                            var tbody = document.createElement("tbody")
                            var ids = "";
                            for (var i = 0; i < str.length; i++) {
                                var tr = document.createElement("tr");
                                for (var key in str[i]) {
                                    var td = document.createElement("td");
                                    td.innerHTML = str[i][key];
                                    tr.appendChild(td);

                                }
                                tbody.appendChild(tr);

                            }
                            var parNode = document.getElementById("tabletofill");
                            parNode.appendChild(tbody);
                            //alert(ids);






                        } else {
                            alert("The result is failed!");
                        }
                    },

                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert("系统繁忙,请稍候");
                },
                //请求完成后回调函数 (请求成功或失败之后均调用)。参数： XMLHttpRequest 对象和一个描述成功请求类型的字符串
                complete: function (XMLHttpRequest, textStatus) {
                    //this 调用本次AJAX请求时传递的options参数
                },
                //一组数值的HTTP代码和函数对象，当响应时调用了相应的代码。例如，如果响应状态是404，将触发以下警报：
                statusCode: {
                    404: function () {
                        alert('404，页面不存在');
                    }
                }
            });



        }
        function Export() {
            var time = document.getElementById("export").value;
            $.ajax({
                url: "/RTData/ExportExcelToClient",  //ajax请求地址
                type: "POST",//请求方式 "POST" 或 "GET"， 默认为 "GET"。注意：其它 HTTP 请求方法，如 PUT 和 DELETE 也可以使用，但仅部分浏览器支持。
                //dataType: "File",    //根据返回数据类型可以有这些类型可选：xml html script json jsonp text
                //发送到服务器的数据，可以直接传对象{a:0,b:1}，如果是get请求会自动拼接到url后面，如：&a=0&b=1
                //如果为数组，jQuery 将自动为不同值对应同一个名称。如 {foo:["bar1", "bar2"]} 转换为 "&foo=bar1&foo=bar2"。
                data: { tablename: $("#name").val(), date: time },
                success:
                    function (result) {

                        if (!result.IsSuccess) {
                            alert("ok");
                            //返回的json的字符串
                            //alert(result.json);
                            






                        } else {
                            alert("The result is failed!");
                        }
                    },

                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert("系统繁忙,请稍候");
                },
                //请求完成后回调函数 (请求成功或失败之后均调用)。参数： XMLHttpRequest 对象和一个描述成功请求类型的字符串
                complete: function (XMLHttpRequest, textStatus) {
                    //this 调用本次AJAX请求时传递的options参数
                },
                //一组数值的HTTP代码和函数对象，当响应时调用了相应的代码。例如，如果响应状态是404，将触发以下警报：
                statusCode: {
                    404: function () {
                        alert('404，页面不存在');
                    }
                }
            });

        }
    </script>
}

